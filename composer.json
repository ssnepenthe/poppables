{
    "name": "ssnepenthe/poppables",
    "description": "Attempts to normalize the Pimple DI container to the PSR container interface",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Poppables\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Poppables\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.3 || ^8.0",
        "pimple/pimple": "^3.5",
        "psr/container": "^1.1 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    }
}
